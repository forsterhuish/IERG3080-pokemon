<Window x:Class="IERG3080PartII.renameWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IERG3080PartII"
        mc:Ignorable="d"
        Title="renameWin" Height="122" Width="203" ResizeMode="NoResize">
    <Grid>
        <TextBlock HorizontalAlignment="Left" Margin="10,13,0,0" Text="Old Name:" TextWrapping="Wrap" VerticalAlignment="Top"/>
        <TextBox x:Name="Input_Box" HorizontalAlignment="Left" Margin="81,0,0,0" TextWrapping="Wrap" VerticalAlignment="Center" Width="97"/>
        <TextBlock x:Name="OldName_Block" HorizontalAlignment="Left" Margin="81,13,0,0" TextWrapping="Wrap" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="10,0,0,0" Text="New Name:" TextWrapping="Wrap" VerticalAlignment="Center"/>
        <Button x:Name="submitButton" Content="Submit" HorizontalAlignment="Left" Margin="90,67,0,0" VerticalAlignment="Top" Click="submitButton_Click"/>

    </Grid>
</Window>
